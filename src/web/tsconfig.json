{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "strict": true,
    "outDir": "dist",
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true

  },
  "include": ["./**/*"],
}
